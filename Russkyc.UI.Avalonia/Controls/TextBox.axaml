        </Style>
        <!--  Show Clear Button State  -->
        <Style Selector="^.Clearable[AcceptsReturn=False][IsReadOnly=False]:focus:not(TextBox:empty)">
            <Setter Property="InnerRightContent">
                <Template>
                    <Button
                        Width="28"
                        Height="28"
                        Margin="0,0,4,0"
                        VerticalAlignment="Center"
                        Classes="Ghost"
                        ClipToBounds="True"
                        Command="{Binding $parent[TextBox].Clear}"
                        Theme="{DynamicResource TextBoxButton}">
                        <Image
                            Width="12"
                            Height="12"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center">
                            <Image.Resources>
                                <Pen
                                    x:Key="IconPen"
                                    Brush="{DynamicResource MutedForegroundBrush}"
                                    Thickness="1.5" />
                            </Image.Resources>
                            <DrawingImage>
                                <DrawingGroup>
                                    <GeometryDrawing Geometry="F1 M18 6L6 18" Pen="{DynamicResource IconPen}" />
                                    <GeometryDrawing Geometry="F1 M6 6L18 18" Pen="{DynamicResource IconPen}" />
                                </DrawingGroup>
                            </DrawingImage>
                        </Image>
                    </Button>
                </Template>
            </Setter>
        </Style>
    </ControlTheme>
</ResourceDictionary>